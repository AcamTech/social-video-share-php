{
    "name": "TorCDN/SocialVideoShare",
    "description": "Stream video from S3/URL and upload/share to Facebook, Youtube, Twitter and Vimeo",
    "keywords": ["aws", "S3", "video", "share", "stream", "facebook", "youtube", "twitter", "vimeo"],
    "homepage": "http://torcdn.com/?social-video-share",
    "license": "MIT",
    "scripts": {
        "test": "./vendor/bin/phpunit test/"
    },
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "google/apiclient": "2.0",
        "aws/aws-sdk-php": "3.*",
        "vimeo/vimeo-api": "^1.3",
        "jublonet/codebird-php": "^3.1",
        "silex/silex": "~2.0",
        "facebook/graph-sdk": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": { 
            "TorCDN\\SocialVideoShare\\": "src/SocialVideoShare",
            "TorCDN\\Server\\": "src/Server"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "TorCDN\\SocialVideoShare\\Test\\" : "test"
        }
    }
}
